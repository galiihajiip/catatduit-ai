# Redis
REDIS_URL=redis://localhost:6379

# AI
AI_CONFIDENCE_THRESHOLD=0.85

# OCR
TESSERACT_CMD=/usr/bin/tesseract
OCR_CONFIDENCE_THRESHOLD=0.85
MAX_UPLOAD_SIZE=10485760

# App
DEBUG=false
