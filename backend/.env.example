# Database
DATABASE_URL=postgresql+asyncpg://postgres:password@localhost:5432/catatduit

# JWT
SECRET_KEY=your-super-secret-key-change-in-production
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# Telegram
TELEGRAM_BOT_TOKEN=your-telegram-bot-token
